{
  "manifest_version": 2,
  "name": "Rokot HTTP proxy",
  "version": "1.5",
  "description": "Proxy",
  "background": {
    "scripts": ["js/main.js"],
    "type": "module"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "test-addon-id@rokot",
      "strict_min_version": "113.0"
    },
    "gecko_android": {
      "id": "test-addon-id@rokot",
      "strict_min_version": "113.0"
    }
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["proxy", "storage", "<all_urls>"]
}
